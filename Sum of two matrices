#include<stdio.h>

int main()
{
    int i,j,a[3][2],b[3][2],sumM[3][2];
    
    for(i=0;i<3;i++){
        for(j=0;j<2;j++){
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<3;i++){
        for(j=0;j<2;j++){
            scanf("%d",&b[i][j]);
        }
    }
    for(i=0;i<3;i++){
        for(j=0;j<2;j++){
            printf("%d",a[i][j]);
        }
        printf("\n");
    }
    printf("\n");
    for(i=0;i<3;i++){
        for(j=0;j<2;j++){
            printf("%d",b[i][j]);
        }
        printf("\n");
    }
    for(i=0;i<3;i++){
        for(j=0;j<2;j++){
            sumM[i][j]=a[i][j]+b[i][j];
        }
      printf("\n");  
    }
    for(i=0;i<3;i++){
        for(j=0;j<2;j++){
            printf("%d ",sumM[i][j]);
        }
        printf("\n");
    }
}
